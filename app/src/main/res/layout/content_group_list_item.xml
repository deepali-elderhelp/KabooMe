                    <FrameLayout
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        app:layout_constraintStart_toEndOf="@id/guideline_conv_name_end"
                        app:layout_constraintEnd_toEndOf="@id/guideline_expiry_details"
                        app:layout_constraintTop_toTopOf="@id/gr_li_fr_list_item_group_name_constraint"
                        android:layout_gravity="bottom"
                        android:id="@+id/gr_li_fr_list_item_group_requests"
                        android:visibility="visible"
                        >
                        <ImageView
                            android:id="@+id/requests_image"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:src="@drawable/requests"
                            android:layout_gravity="bottom"
                            ></ImageView>

                        <TextView
                            android:id="@+id/gr_li_fr_list_item_group_requests_count"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textColor="#fff"
                            android:textSize="15sp"
                            android:text="0"
                            android:textAlignment="viewStart"
                            android:layout_marginLeft="5dp"
                            />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/requests_small"
                            android:layout_gravity="end"/>


                    </FrameLayout>

    <FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/gr_li_fr_list_item_unread_pm_count"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_unread_details_middle"
        app:layout_constraintEnd_toEndOf="@id/guideline_group_list_item_vertical_end"
        app:layout_constraintBottom_toBottomOf="@id/guideline_group_list_item_horizontal_end"
        app:layout_constraintTop_toTopOf="@id/guideline_group_list_item_horizontal_middle_end"
        >
        <ImageView
            android:id="@+id/unread_pm_image"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:src="@drawable/shield"
            ></ImageView>
        <ImageView
            android:id="@+id/unread_pm_image_circle"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/admin_dots"
            ></ImageView>
        <TextView
            android:id="@+id/gr_li_fr_list_item_unread_pm_count_number"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="#fff"
            android:textSize="15sp"
            android:text="0"
            android:textAlignment="center"
            />

    </FrameLayout>

                    <FrameLayout
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        android:id="@+id/gr_li_fr_list_item_unread_count"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toEndOf="@id/guideline_unread_details"
