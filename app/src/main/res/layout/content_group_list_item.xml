            <TextView
                android:id="@+id/gr_li_fr_list_item_group_expiry"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="@id/guideline_expiry_details"
                app:layout_constraintEnd_toEndOf="@id/guideline_group_list_item_vertical_end"
                app:layout_constraintTop_toTopOf="@id/gr_li_fr_list_item_group_name_constraint"
                android:text="Expires on 22nd Sept"
                style="@style/expiry_warning"
                android:gravity="bottom"

                />

            <TextView
                android:id="@+id/gr_li_fr_list_item_group_message"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toEndOf="@id/guideline_conv_user_action_vertical_middle"
                app:layout_constraintEnd_toEndOf="@id/guideline_expiry_details"
                app:layout_constraintTop_toBottomOf="@id/gr_li_fr_list_item_group_name_constraint"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginLeft="10dp"
                android:paddingBottom="5dp"
                android:maxLines="2"
                android:ellipsize="end"
                style="@style/group_list_last_message"
