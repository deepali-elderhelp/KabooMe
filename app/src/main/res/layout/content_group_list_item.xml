<androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/gr_li_fr_list_item_parent_layout">

    <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_group_list_item_vertical_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="16dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_group_list_item_vertical_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_end="16dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_group_list_item_horizontal_start"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_group_list_item_horizontal_middle_end"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.60"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_group_list_item_horizontal_end"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_conv_user_action_vertical_middle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.22"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_expiry_details"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.7"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_conv_name_end"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.6"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_unread_details"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.82"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_unread_details_middle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.894"/>

    <FrameLayout
        android:id="@+id/gr_li_fr_list_item_group_image_frame"
        android:layout_width="@dimen/user_group_list_image_width"
        android:layout_height="@dimen/user_group_list_image_height"
        app:layout_constraintStart_toStartOf="@id/guideline_group_list_item_vertical_start"
        app:layout_constraintEnd_toEndOf="@id/guideline_conv_user_action_vertical_middle"
        app:layout_constraintTop_toTopOf="@id/guideline_group_list_item_horizontal_start"
        app:layout_constraintBottom_toBottomOf="@id/guideline_group_list_item_horizontal_end">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/gr_li_fr_list_item_group_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
             />

<!--        <com.java.kaboome.presentation.views.widgets.GlideImageView-->
<!--            android:id="@+id/gr_li_fr_list_item_group_image"-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="match_parent"-->
<!--            />-->

        <ProgressBar
            android:id="@+id/gr_li_fr_list_item_group_image_progress"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:indeterminateDrawable="@drawable/progress"
            android:indeterminateDuration="1000"
            android:visibility="visible"/>
    </FrameLayout>



