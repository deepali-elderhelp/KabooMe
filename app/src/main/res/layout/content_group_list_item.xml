                            ></ImageView>
                        <TextView
                            android:id="@+id/gr_li_fr_list_item_unread_count_number"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textColor="#fff"
                            android:textSize="15sp"
                            android:text="0"
                            android:textAlignment="center"
                            />

                    </FrameLayout>

